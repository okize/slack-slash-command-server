{
  "name": "slack-slash-server",
  "version": "0.0.0",
  "private": true,
  "description": "Server for responding to custom Slack slash commands",
  "engines": {
    "node": "4.2.6"
  },
  "scripts": {
    "start": "node --use_strict ./bin/www",
    "start:debug": "DEBUG=slack-slash-server node --use_strict ./bin/www",
    "watch": "DEBUG=slack-slash-server nodemon --use_strict ./bin/www",
    "lint": "eslint --ext .js,.jsx . || true"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/okize/slack-slash-server"
  },
  "author": "Morgan Wigmanich <okize123@gmail.com> (https://github.com/okize)",
  "homepage": "https://github.com/okize/slack-slash-server",
  "bugs": "https://github.com/okize/slack-slash-server/issues",
  "license": "MIT",
  "dependencies": {
    "body-parser": "~1.14.1",
    "compression": "~1.6.1",
    "cookie-parser": "~1.3.5",
    "debug": "~2.2.0",
    "express": "~4.13.3",
    "flip": "~0.1.1",
    "jade": "~1.11.0",
    "morgan": "~1.6.1",
    "request": "~2.67.0",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "eslint": "~1.10.0",
    "eslint-config-airbnb": "~4.0.0",
    "eslint-plugin-react": "~3.16.0",
    "nodemon": "~1.8.1"
  }
}
